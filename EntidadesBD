Clientes(CI(int, PK, int, auto_increment), Nombre(VARCHAR(25), NOT NULL), Apellido(VARCHAR(25), NOT NULL), Teléfono(VARCHAR(18)), Contraseña(Varchar(25), NOT NULL), Fecha_Registro(DATE), Ultimo_Acceso(DATE), Activo(boolean)
Organizador(ID(int, PK, auto_increment), Verificado(boolean), Nombre(VARCHAR(25), NOT NULL), Apellido(VARCHAR(25),  NOT NULL), Teléfono(VARCHAR(18)), Contraseña(VARCHAR(25), NOT NULL), Num_Fiscal(CHAR(11))
Evento(ID_Evento(int, PK, auto_increment), Título(VARCHAR(50), NOT NULL), Descripción(VARCHAR(100), NOT NULL), Fecha_Inicio(DATETIME, NOT NULL), Fecha_Fin(DATETIME, NOT NULL),Ubicacion(VARCHAR(50), NOT NULL), Capacidad(INT, UNSIGNED, NOT NUL), Precio(int, NOT NULL) Creado_Por(VARCHAR(25)), Estado(ENUM('borrador', 'pendiente', 'publicado', 'cancelado', 'completado') DEFAULT 'borrador', NOT NULL), Feacha_Creación(DATETIME DEFAULT CURRENT_TIMESTAMP)
Administrador(ID_Administrador(int, PK, auto_increment), Nombre(VARCHAR(25), NOT NULL), Apellido(VARCHAR(25), NOT NULL), Correo(VARCHAR(39), NOT NULL), Contraseña(VARCHAR(25), NOT NULL), Teléfono(Varchar(18), NOT NULL), Activo(boolean)
Comentario(ID_Comentario(int, PK, auto_increment),CI(FK),ID_Evento(FK), Texto(VARCHAR(100)), Valoracion(TINYINT CHECK(Valoracion BETWEEN 1 AND 5), Estado(ENUM('borrador', 'pendiente', 'publicado', 'cancelado', 'completado') DEFAULT 'borrador')
Categoría(ID_Categoría(int, PK, auto_increment), Nombre(VARCHAR(12), NOT NULL), Descripción(VARCHAR(100))
