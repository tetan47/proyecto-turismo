Clientes(CI(INT, PK, AUTO_INCREMENT), Nombre(VARCHAR(25), NOT NULL), Apellido(VARCHAR(25), NOT NULL), Teléfono(VARCHAR(18)), Contraseña(VARCHAR(25), NOT NULL), Fecha_Registro(DATE), Ultimo_Acceso(DATE), Activo(BOOLEAN);
Organizador(ID(INT, PK, AUTO_INCREMENT), Verificado(BOOLEAN), Nombre(VARCHAR(25), NOT NULL), Apellido(VARCHAR(25),  NOT NULL), Teléfono(VARCHAR(18)), Contraseña(VARCHAR(25), NOT NULL), Num_Fiscal(VARCHAR(11));
Evento(ID_Evento(INT, PK, AUTO_INCREMENT), Título(VARCHAR(50), NOT NULL), Descripción(VARCHAR(100), NOT NULL), Fecha_Inicio(DATETIME, NOT NULL), Fecha_Fin(DATETIME, NOT NULL),Ubicacion(VARCHAR(50), NOT NULL), Capacidad(INT, UNSIGNED, NOT NUL), Precio(INT, NOT NULL) Creado_Por(VARCHAR(25)), Estado(ENUM('borrador', 'pendiente', 'publicado', 'cancelado', 'completado') DEFAULT 'borrador', NOT NULL), Fecha_Creación(DATETIME DEFAULT CURRENT_TIMESTAMP);
Administrador(ID_Administrador(INT, PK, AUTO_INCREMENT), Nombre(VARCHAR(25), NOT NULL), Apellido(VARCHAR(25), NOT NULL), Correo(VARCHAR(39), NOT NULL), Contraseña(VARCHAR(25), NOT NULL), Teléfono(VARCHAR(18), NOT NULL), Activo(BOOLEAN);
Comentario(ID_Comentario(INT, PK, AUTO_INCREMENT),CI(FK),ID_Evento(FK), Texto(VARCHAR(100)), Valoracion(TINYINT CHECK(Valoracion BETWEEN 1 AND 5), Estado(ENUM('borrador', 'pendiente', 'publicado', 'cancelado', 'completado') DEFAULT 'borrador');
Categoría(ID_Categoría(INT, PK, AUTO_INCREMENT), Nombre(VARCHAR(12), NOT NULL), Descripción(VARCHAR(100));
